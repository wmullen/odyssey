{
    "id": "odyssey",
    "title": "Odyssey",
    "description": "The Odyssey role-playing system for FoundryVTT.",
    "version": "0.6.4",
    "compatibility": {
      "minimum": "12",
      "verified": "12"
    },
    "authors": [
      {
        "name": "Raetoria"
      }
    ],
    "esmodules": ["./odyssey.mjs"],
    "styles": ["styles/odyssey.css"],
    "packs": [],
    "languages": [
      {
        "lang": "en",
        "name": "English",
        "path": "lang/en.json"
      }
    ],
    "documentTypes": {
      "Actor": {
        "playercharacter": { "htmlFields": [
          "themebooks.themebook0.basicMove",
          "themebooks.themebook0.abilityList",
          "themebooks.themebook1.basicMove",
          "themebooks.themebook1.abilityList",
          "themebooks.themebook2.basicMove",
          "themebooks.themebook2.abilityList",
          "themebooks.themebook3.basicMove",
          "themebooks.themebook3.abilityList",
          "signatureMove.signatureOngoing",
          "signatureMove.signatureActive",
          "signatureMove.signatureEffect",
          "signatureMove.signatureFrequency",
          "signatureMove.signatureDownside",
          "items",
          "background.pronouns",
          "background.biography",
          "notes"
        ] },
        "npc": { "htmlFields": ["background.biography"] }
      },
      "Item": {
        "equipment": {},
        "themebook": {}
      }
    },
    "socket": false,
    "gridDistance": 5,
    "gridUnits": "ft",
    "primaryTokenAttribute": "attributes.harm",
    "secondaryTokenAttribute": null,
    "url": "https://github.com/wmullen/odyssey",
    "manifest": "https://raw.githubusercontent.com/wmullen/odyssey/main/system.json"
}




